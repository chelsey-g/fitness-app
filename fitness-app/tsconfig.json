{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/competitions/[name]/page.jsx", "app/api/send/route.js", "app/tracker/chart/page.jsx", "app/tracker/page.jsx", "app/workouts/exercise/[name]/page.jsx", "app/workouts/list/[name]/page.jsx", "app/workouts/page.jsx", "components/AddList.jsx", "components/AddPlayers.jsx", "components/CompetitionsActions.jsx", "components/CompetitonForm.jsx", "components/WorkOutModal.jsx", "components/WorkoutLists.jsx", "components/WorkoutListActions.jsx", "components/WorkoutActions.jsx", "components/WeightGraph.jsx", "components/TrackerActions.jsx", "components/Search.jsx", "components/ProgressTracker.jsx", "components/Header.jsx", "components/DifficultyFilter.jsx", "components/DateRangePicker.jsx", "components/CreateWorkout.jsx"],
  "exclude": ["node_modules"]
}
