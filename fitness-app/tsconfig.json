{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/competitions/[name]/page.jsx", "app/api/send/route.js", "app/tracker/chart/page.tsx", "app/tracker/page.tsx", "app/workouts/exercise/[name]/page.tsx", "app/workouts/list/[name]/page.tsx", "app/workouts/page.tsx", "components/AddList.tsx", "components/AddPlayers.tsx", "components/CompetitionsActions.tsx", "components/CompetitonForm.tsx", "components/WorkOutModal.tsx", "components/WorkoutLists.tsx", "components/WorkoutListActions.tsx", "components/WorkoutActions.tsx", "components/WeightGraph.tsx", "components/TrackerActions.tsx", "components/Search.jsx", "components/ProgressTracker.tsx", "components/Header.jsx", "components/DifficultyFilter.tsx", "components/DateRangePicker.tsx", "components/CreateWorkout.tsx", "app/contact/page.tsx", "app/workouts/browse/page.tsx", "components/Navigation.tsx", "app/functions.tsx", "vitest.config.mts"],
  "exclude": ["node_modules"]
}
